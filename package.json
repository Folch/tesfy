{
  "name": "testfy",
  "version": "1.0.0",
  "author": "Andres Alvarez <a.alvarez.sor@gmail.com>",
  "description": "A lightweight A/B Testing and Feature Flag JavaScript library focused on performance",
  "keywords": [
    "A/B Testing",
    "Feature Flags",
    "Experiment",
    "TypeScript",
    "JavaScript"
  ],
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build:esm": "tsc --module es2015 --outDir dist/esm --declaration false",
    "build:umd": "webpack",
    "clean": "rimraf dist",
    "build": "yarn clean && yarn build:esm && yarn build:umd",
    "test": "jest",
    "lint": "yarn eslint . --ext .ts",
    "size": "yarn build && size-limit"
  },
  "libraryConfig": {
    "name": "Testfy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andresz1/testfy.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andresz1/testfy/issues"
  },
  "homepage": "https://github.com/andresz1/testfy#readme",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.4.2",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "prettier": "^2.0.2",
    "rimraf": "^2.6.2",
    "size-limit": "^4.4.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^4.4.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2",
    "webpack": "^4.4.16",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "json-logic-js": "^1.2.2",
    "lodash": "^4.17.15",
    "murmurhash": "^0.0.2"
  }
}
